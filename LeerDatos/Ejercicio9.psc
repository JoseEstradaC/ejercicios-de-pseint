Algoritmo Ejercicio9
	Definir Radio, Altura, Total Como Real;
	
	Escribir Sin Saltar "Por favor introduzca el radio";
	Leer Radio;
	Escribir Sin Saltar "Por favor introduzca la altura";
	Leer Altura;
	
	Total <- 1 / 3 * PI * Radio * Radio * Altura;
	
	Escribir "El volumen del cono es ", Total;
	
FinAlgoritmo