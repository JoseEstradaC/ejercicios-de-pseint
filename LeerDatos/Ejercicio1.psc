Algoritmo Ejercicio1
	Definir Resultado, Multiplicador1, Multiplicador2 Como Real;
	Escribir Sin Saltar "Por favor, intruduzca el primer multiplicador: ";
	Leer Multiplicador1;
	Escribir Sin Saltar "Por favor, intruduzca el segundo multiplicador: ";
	Leer Multiplicador2;
	Resultado <- Multiplicador1 * Multiplicador2;
	Escribir "El resultado es: ", Resultado;
FinAlgoritmo
